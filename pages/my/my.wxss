page{
  background: #F2F2F2;
}
.my_nav{
  width: 95%;
  height: 538rpx;
  background: white;
  border:1px #D7D7D7 solid;
  border-radius: 30rpx;
  margin: 60rpx auto;
}
.nav_top{
  width:100%;
  height:133rpx;
  border-bottom: 1px #D7D7D7 solid;
}
.nav_about{
  border: none;
}
.nav_user>view{
  width: 140rpx;
  height: 130rpx;
  float: left;
  margin-left: 20rpx;
}
.nav_user>view:nth-of-type(1)>image{
  width:100rpx;
  height:100rpx;
  border-radius: 50%;
  overflow: hidden;
  border: 1px black solid;
  margin: 10rpx;
}
.nav_user>view:nth-of-type(2){
  width: 400rpx;
  height: 130rpx;
  background-color: white;
}
.nav_user>view>text{
  display: block;
  width: 400rpx;
  float: left;
  font-size: 30rpx;
  margin-top: 20rpx;
}
.nav_user>view>text:nth-of-type(2){
  font-size: 22rpx;
}
.nav_user>view:nth-of-type(3){
  width: 100rpx;
}
.nav_user>view:nth-of-type(3)>image{
  width: 40rpx;
  height: 50rpx;
  margin-top: 40rpx;
  float: right;
}
.nav_left{
  width: 100rpx;
  height: 130rpx;
  float: right;
margin-right: 20rpx;

}
.nav_left>image{
 width: 40rpx;
  height: 50rpx;
  margin-top: 40rpx;
  float: right;
}
.nav_text>text{
  line-height: 133rpx;
  font-weight: bold;
  font-size: 34rpx;
  margin-left: 52rpx;
}
.content{
  width: 95%;
  height: 250rpx;
  border: 1px #F2F2F2 solid;
  background: #FFFFFF;
  margin: 10rpx auto;
}
.content>view{
  width: 49%;
  height: 250rpx;
  text-align: center;
  border-right: 1px #F2F2F2 solid;
  float: left;
}
.content>view>text{
  display: inline-block;
  width: 300rpx;
  font-size: 34rpx;
  font-weight: bold;
  margin-top: 30rpx;
}
.content>view>text:nth-of-type(1){
  margin-top: 60rpx;
}
.content>view:nth-of-type(2){
  border: none;
}